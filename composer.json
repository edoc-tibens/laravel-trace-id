{
    "name": "edoc-tibens/laravel-trace-id",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Edoc\\LaravelTraceId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tim Bensberg",
            "email": "tim.bensberg@edoc.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.9"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze --memory-limit=2G",
        "test": "vendor/bin/phpunit"
    }
}
